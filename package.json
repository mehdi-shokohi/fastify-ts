{
  "name": "fastify-ts",
  "version": "1.0.0",
  "description": "fastify ts",
  "main": "dist/src/index.js",
  "bin": "dist/main.js",
  "scripts": {
    "start:dev": "webpack --mode=development --node-env=development",
    "run:dev": "NODE_ENV=development node dist/main.js",
    "start": "ts-loader ./src/index.ts",
    "build": "webpack --mode=production --node-env=production ",
    "execBundle" :"pkg dist/main.js --out-path=exeBundle",
    "json2ts": "json2ts -i schemas -o src/types",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@webpack-cli/generators": "^2.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.19.0",
    "json-schema-to-ts": "^1.6.5",
    "json-schema-to-typescript": "^10.1.5",
    "pkg": "^5.7.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.7.0",
    "typedoc": "^0.5.10",
    "typedoc-webpack-plugin": "^1.1.4",
