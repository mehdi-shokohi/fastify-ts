{
  "name": "fastify-ts-api",
  "version": "0.0.1",
  "description": "fastify sample api",
  "main": "dist/src/index.js",
  "bin": "dist/src/index.js",
  "scripts": {
    "start:dev": "ts-node src/index.ts",
    "start": "node dist/src/index.js",
    "build": "mkdir -p ./dist && tsc && cp .env ./dist && json2ts -i dist/schemas -o dist/src/types",
    "json2ts": "json2ts -i schemas -o src/types",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "json-schema-to-ts": "^1.6.5",
    "json-schema-to-typescript": "^10.1.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-cors": "^6.0.3",
    "fastify-file-upload": "^3.0.1",
    "fastify-formbody": "^5.2.0",
    "fastify-helmet": "^7.0.1",
    "fastify-plugin": "^3.0.1",
    "node-async-redis": "^1.0.1",
    "winston": "^3.8.1"
  }
}
